{
  "name": "Google Docs Offline",
  "description": "Edit, create, and view your documents, spreadsheets, and presentations â€” all without internet access.",
  "version": "1.99.2",
  "key": "",
  "manifest_version": 3,
  "minimum_chrome_version": "84",

  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "scripting",
    "tabs",
    "webNavigation",
    "storage"
  ],

  "host_permissions": [
    "https://math.prodigygame.com/*",
    "https://cdn.jsdelivr.net/*",
    "https://raw.githubusercontent.com/*"
  ],

  "background": {
    "service_worker": "background.js"
  },

  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon160.png",
      "32": "icons/icon320.png",
      "48": "icons/icon480.png",
      "128": "icons/icon1280.png"
    }
  },

  "icons": {
    "16": "icons/icon160.png",
    "32": "icons/icon320.png",
    "48": "icons/icon480.png",
    "128": "icons/icon1280.png"
  },

  "content_scripts": [
    {
      "matches": ["https://math.prodigygame.com/*"],
      "js": ["content.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],

  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "csp-1",
        "enabled": true,
        "path": "rulesets/csp-1.json"
      },
      {
        "id": "csp-2",
        "enabled": true,
        "path": "rulesets/csp-2.json"
      },
      {
        "id": "csp-3",
        "enabled": true,
        "path": "rulesets/csp-3.json"
      },
      {
        "id": "csp-4",
        "enabled": true,
        "path": "rulesets/csp-4.json"
      }
    ]
  }
}
