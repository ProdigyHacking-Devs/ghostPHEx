{
  "name": "Google Docs Offline",
  "description": "Edit, create, and view your documents, spreadsheets, and presentations â€” all without internet access.",
  "version": "1.4",
  "manifest_version": 3,
  "minimum_chrome_version": "84",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "scripting",
    "tabs",
    "webNavigation",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon160.png",
      "32": "icons/icon320.png",
      "48": "icons/icon480.png",
      "128": "icons/icon1280.png"
    }
  },
  "icons": {
    "16": "icons/icon160.png",
    "32": "icons/icon320.png",
    "48": "icons/icon480.png",
    "128": "icons/icon1280.png"
  }
}
